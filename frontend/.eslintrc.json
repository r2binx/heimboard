{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    // override/add rules settings here, such as:
    // 'vue/no-unused-vars': 'error'
    // "no-undef": "warn",
    // "no-unused-vars": "warn",
    // "vue/multi-word-component-names": "off",
  }
}
